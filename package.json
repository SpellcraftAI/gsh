{
  "name": "@gptlabs/gsh",
  "version": "1.1.0",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "platform": "node",
  "engines": {
    "node": ">=14"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "gsh": "dist/bin.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.txt"
  ],
  "scripts": {
    "dev": "tsmodule dev",
    "build": "tsmodule build --standalone \"src/{bin.ts,header.txt}\"",
    "lint": "eslint --fix",
    "pretest": "yarn build -d && npm link -f",
    "test": "cross-env NODE_ENV=test yarn ava",
    "update-tests": "cross-env NODE_ENV=test yarn ava",
    "release": "release-it --ci -VV",
    "canary": "release-it --preRelease=canary --ci -VV",
    "prepublishOnly": "yarn build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@tsmodule/log": "^2.2.1",
    "auth0-node-client": "5.2.1-cjs.0",
    "commander": "^10.0.1",
    "debug-logging": "^4.1.2",
    "universal-shell": "^35.3.0"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^44.7.0",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "ava": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "openai": "^3.2.1",
    "typescript": "^5.0.4"
  }
}
