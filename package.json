{
  "name": "@gptlabs/gsh",
  "version": "0.0.6",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "platform": "node",
  "engines": {
    "node": ">=14"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "gsh": "dist/bin.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.txt"
  ],
  "scripts": {
    "dev": "tsmodule dev",
    "build": "tsmodule build --standalone \"src/{bin.ts,header.txt}\"",
    "postinstall": "cp ../node_modules/gpt-3-encoder/encoder.json dist/encoder.json && cp ../node_modules/gpt-3-encoder/vocab.bpe dist/vocab.bpe",
    "lint": "eslint --fix",
    "pretest": "yarn build -d && npm link -f",
    "test": "cross-env NODE_ENV=test yarn ava",
    "update-tests": "cross-env NODE_ENV=test yarn ava",
    "release": "release-it --ci -VV",
    "canary": "release-it --preRelease=canary --ci -VV",
    "prepublishOnly": "yarn build"
  }
}
