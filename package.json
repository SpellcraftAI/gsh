{
  "name": "@spellcraft/gsh",
  "version": "1.4.1-canary.4",
  "license": "SEE LICENSE IN LICENSE",
  "type": "commonjs",
  "platform": "node",
  "engines": {
    "node": ">=14"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "gsh": "dist/bin.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.txt"
  ],
  "scripts": {
    "dev": "tsmodule dev",
    "build": "tsmodule build --binary",
    "lint": "eslint --fix",
    "test": "pnpm build && cross-env NODE_ENV=test pnpm ava",
    "release": "release-it --ci -VV",
    "canary": "release-it --preRelease=canary --ci -VV",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@tsmodule/log": "^2.2.1",
    "auth0-node-client": "5.2.1-cjs.0",
    "commander": "^10.0.1",
    "debug-logging": "^4.1.2",
    "universal-shell": "^35.4.0"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^44.7.0",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "ava": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "openai": "^3.2.1",
    "pkg": "^5.8.1",
    "typescript": "^5.0.4"
  },
  "release-it": {
    "git": {
      "commitMessage": "release: v${version}",
      "tagAnnotation": "v${version}"
    },
    "github": {
      "release": true,
      "comments": {
        "submit": true
      },
      "assets": [
        "dist/bin-*"
      ],
      "releaseName": "v${version}"
    }
  }
}